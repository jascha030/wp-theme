{
  "name": "jascha030/composer-template",
  "description": "Template repo for creating composer projects",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jascha030\\WpTheme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jascha030\\WpTheme\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "symfony/filesystem": "^5.3",
    "symfony/finder": "^5.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpunit/phpunit": "^9.5"
  },
  "authors": [
    {
      "name": "Jascha030",
      "email": "contact@jaschavanaalst.nl"
    }
  ],
  "keywords": [
    "WordPress",
    "theme",
    "wp",
    "minimal"
  ],
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "phpunit": "vendor/bin/phpunit",
    "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
  }
}
